{
    "llm": {
        "providers": {
            "nvidia": {
                "active": true
            }
        },
        "fallback_order": [
            "nvidia"
        ],
        "defaults": {
            "model": "nvidia/llama3-chatqa-1.5-8b",
            "temperature": 0.0,
            "max_tokens": 1000
        }
    },
    "algorithm": {
        "decision_thresholds": {
            "confidence_norm_factor": 10,
            "high_confidence_threshold": 0.7,
            "dominant_gap_ratio": 0.3,
            "low_diversity_pairs_threshold": 2,
            "diversity_ratio_threshold": 0.3,
            "sparse_graph_density_threshold": 0.05,
            "path_support_threshold": 2,
            "stability_cycle_threshold": 3,
            "passed_to_total_ratio_threshold": 0.2,
            "minimum_hypotheses_threshold": 1,
            "semantic_similarity_threshold": 0.7
        },
        "signal_params": {
            "positive_threshold": 1.0,
            "negative_threshold": -1.0,
            "reputation_positive_delta": 10,
            "reputation_negative_delta": -20,
            "weights": {
                "passed_hypothesis_count": 1.0,
                "mean_confidence": 0.8,
                "graph_density": 0.5,
                "filtered_to_total_ratio": 0.3
            },
            "max_deltas": {
                "passed_hypothesis_count": 100.0,
                "mean_confidence": 20.0,
                "graph_density": 0.2,
                "filtered_to_total_ratio": 0.5
            }
        },
        "ingestion_defaults": {
            "segmentation_strategy": "sentences",
            "sentences_per_block": 1,
            "enable_lexical_repair": false
        },
        "deduplication_defaults": {
            "algorithm": "sha256",
            "similarity_threshold": 0.95,
            "components": [
                "title",
                "abstract"
            ]
        },
        "path_reasoning_defaults": {
            "max_hops": 4,
            "allow_len3": true
        },
        "domain_resolution": {
            "allowed_domains": [
                "biomedical",
                "computer_science",
                "physics",
                "chemistry",
                "mathematics",
                "engineering"
            ],
            "llm_order": [
                "nvidia"
            ]
        },
        "indirect_path": {
            "enabled": true,
            "temporal_placeholders": true,
            "dominance_gap_threshold": 0.2,
            "min_length": 3,
            "max_length": 4
        }
    },
    "query_orchestrator": {
        "signature_length": 64,
        "initial_reputation": 0,
        "exhaustion_decay": -5,
        "max_reuse_attempts": 3,
        "fetch_batch_size": 3,
        "results_limit": 10,
        "top_k_hypotheses": 1,
        "min_reputation": -10,
        "fetch_params": {
            "timeout_seconds": 30,
            "retry_attempts": 3
        }
    },
    "fetch_apis": {
        "providers": {
            "semantic_scholar": {
                "active": true
            }
        },
        "domain_provider_order": {
            "biomedical": [
                "semantic_scholar"
            ],
            "computer_science": [
                "semantic_scholar"
            ],
            "physics": [
                "semantic_scholar"
            ],
            "default": [
                "semantic_scholar"
            ]
        }
    },
    "prompt_assets": {
        "domain_resolver": "domain_resolver.txt",
        "triple_extraction": "triple_extraction.txt",
        "decision_llm": "decision_llm.txt",
        "clarification_question": "clarification_question.txt",
        "user_text_classifier": "user_text_classifier.txt",
        "clarification_high": "clarification_high_ambiguity.txt",
        "clarification_medium": "clarification_medium_ambiguity.txt",
        "clarification_low": "clarification_low_ambiguity.txt"
    },
    "decision_provider": "rule_based"
}