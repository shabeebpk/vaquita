{
    "llm": {
        "providers": {
            "nvidia": {
                "active": true
            }
        },
        "fallback_order": [
            "nvidia"
        ],
        "defaults": {
            "model": "nvidia/llama3-chatqa-1.5-8b",
            "temperature": 0.0,
            "max_tokens": 1000
        }
    },
    "algorithm": {
        "decision_thresholds": {
            "confidence_norm_factor": 10,
            "high_confidence_threshold": 0.7,
            "dominant_gap_ratio": 0.3,
            "low_diversity_pairs_threshold": 2,
            "diversity_ratio_threshold": 0.3,
            "sparse_graph_density_threshold": 0.05,
            "path_support_threshold": 2,
            "stability_cycle_threshold": 3,
            "min_absolute_growth_threshold": 0.5,
            "min_relative_growth_threshold": 0.1,
            "passed_to_total_ratio_threshold": 0.2,
            "minimum_hypotheses_threshold": 1,
            "semantic_similarity_threshold": 0.7
        },

        "extraction": {
            "whitelisted_regions": [
                "abstract",
                "introduction",
                "conclusion",
                "results",
                "methods"
            ],
            "excluded_regions": [
                "references",
                "bibliography",
                "acknowledgments",
                "appendix",
                "author_contributions"
            ],
            "enable_zonal_slicing": true,
            "fallback_max_tokens": 1500,
            "column_width_threshold": 200
        },
        "refinery": {
            "model": "nvidia/llama3-chatqa-1.5-8b",
            "prompt_asset": "text_refinery.txt",
            "temperature": 0.0,
            "max_tokens_per_span": 1000,
            "enable_recovery": true,
            "needs_refinement_types": [
                "pdf_text",
                "user_text"
            ]
        },
        "slicing": {
            "strategy": "block",
            "sentences_per_block": 4,
            "max_tokens_per_block": 250,
            "overlap_sentences": 0
        },
        "deduplication_defaults": {
            "algorithm": "sha256",
            "similarity_threshold": 0.95,
            "components": [
                "title",
                "abstract"
            ]
        },
        "path_reasoning_defaults": {
            "max_hops": 3,
            "allow_len3": false,
            "max_indirect_path_length": 3,
            "preferred_predicate_boost_factor": 1.2,
            "hub_degree_threshold": 25,
            "min_confidence": 2
        },
        "domain_resolution": {
            "allowed_domains": [
                "biomedical",
                "computer_science",
                "physics",
                "chemistry",
                "mathematics",
                "engineering"
            ],
            "llm_order": [
                "nvidia"
            ]
        },
        "indirect_path": {
            "enabled": true,
            "temporal_placeholders": true,
            "dominance_gap_threshold": 0.2,
            "min_length": 3,
            "max_length": 3
        },
        "graph_merging": {
            "similarity_threshold": 0.85,
            "min_node_text_length": 2,
            "linkage": "average"
        }
    },
    "embeddings": {
        "default_provider": "sentence_transformers",
        "sentence_transformers": {
            "model_name": "all-MiniLM-L6-v2",
            "batch_size": 32,
            "device": "cpu"
        }
    },
    "caching": {
        "redis": {
            "ttl_seconds": 3600,
            "prefix": "structural_graph:"
        }
    },
    "query_orchestrator": {
        "signature_length": 64,
        "initial_reputation": 0,
        "verification_batch_size": 5,
        "fetch_batch_size": 3,
        "results_limit": 10,
        "top_k_hypotheses": 1,
        "fetch_params": {
            "timeout_seconds": 30,
            "retry_attempts": 3
        }
    },
    "fetch_apis": {
        "providers": {
            "semantic_scholar": {
                "active": true,
                "rate_limit_wait_seconds": 2.0
            }
        },
        "domain_provider_order": {
            "biomedical": [
                "semantic_scholar"
            ],
            "computer_science": [
                "semantic_scholar"
            ],
            "default": [
                "semantic_scholar"
            ]
        }
    },
    "prompt_assets": {
        "domain_resolver": "domain_resolver.txt",
        "triple_extraction": "triple_extraction.txt",
        "decision_llm": "decision_llm.txt",
        "clarification_question": "clarification_question.txt",
        "user_text_classifier": "user_text_classifier.txt",
        "clarification_high": "clarification_high_ambiguity.txt",
        "clarification_medium": "clarification_medium_ambiguity.txt",
        "clarification_low": "clarification_low_ambiguity.txt"
    },
    "input_processing": {
        "classification_threshold": 1000,
        "preview_snippet_length": 300
    },
    "decision_provider": "rule_based",
    "graph_rules": {
        "node_removal_patterns": [
            "^(19|20)\\d{2}$",
            "^(doi:|10\\.\\d+/.*)",
            "^(ISBN|isbn)[\\s-]?(?:10|13)?[\\s-]?[\\d\\s-]+$",
            "^(ISSN|issn)[\\s-]?\\d{4}[\\s-]?\\d{4}$",
            "^https?://|^www\\.",
            "^arxiv:\\d+\\.\\d+$",
            "^(PMID|pmid):\\d+$",
            "^\\d+$",
            "^[a-f0-9\\-]{20,}$",
            "^[^\\w\\s]$",
            "^\\.{2,}$",
            "^[_\\-\\s]*$",
            "^[0-9\\.\\-]{1,3}$"
        ],
        "node_removal_exact": [
            "the",
            "a",
            "an",
            "of",
            "and",
            "or",
            "to",
            "in",
            "is",
            "are",
            "be",
            "by",
            "for",
            "with",
            "as",
            "from",
            "on",
            "at",
            "this",
            "that",
            "which",
            "who",
            "what",
            "where",
            "when",
            "why",
            "how",
            ""
        ],
        "generic_predicates": [
            "related to",
            "mentions",
            "about",
            "involves",
            "concerns",
            "associated with",
            "connected to",
            "linked to"
        ]
    },
    "downloader": {
        "batch_size": 1
    }
}